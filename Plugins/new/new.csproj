<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>$(MSBuildProjectName).Plugin</AssemblyName>
    <BaseOutputPath>$(SolutionDir)\Agent\bin\Debug\Plugins\new</BaseOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Easy.MessageHub" Version="5.0.0" />
    <PackageReference Include="FluentScheduler" Version="5.5.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Sdk">
      <HintPath>..\..\Sdk\bin\Debug\net6.0-windows\Sdk.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
